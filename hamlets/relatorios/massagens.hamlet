<!-- Lista de Usuarios -->
<section style="padding: 200px 0px">
    <div class="container">
        <div class="row">
            <div class="col-lg-12 text-center">
                <h2>Lista de Massagens Realizadas
                <hr class="star-primary">
                <p>&nbsp;

            $maybe mm <- msg                   
                <div class="col-lg-12 text-center">
                    <div class="alert alert-success col-sm-12">
                        <p class="pull-left">#{mm}

            <div class="row">
                <div class="col-lg-8 col-lg-offset-2">
                    <div class="table-responsive">
                         <table class="table table-condensed table-striped table-hover">
                             <thead>
                                <tr>
                                    <th>Massagem NÂº
                                    <th>Nome
                                    <th>Email
                             <tbody>

                                $forall (Entity oq _, Entity _ np, Entity _ fp) <- massagens
                                    <tr class="linha">
                                        <td>#{fromSqlKey oq}
                                        <td>#{usuarioNome np}
                                        <td>#{massagistaNome fp}